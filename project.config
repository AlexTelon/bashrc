[project]
	description = I lost my last bashrc, I have realised I need this!
[access]
	inheritFrom = Private-Projects
[access "refs/*"]
	owner = group user/AlexTelon
[access "refs/for/refs/*"]
	read = group user/AlexTelon
	push = group user/AlexTelon
	label-Code-Review = -2..+2 group user/AlexTelon
	label-Verified = -1..+1 group user/AlexTelon
	rebase = group user/AlexTelon
	removeReviewer = group user/AlexTelon
	submit = group user/AlexTelon
[access "refs/heads/*"]
	read = group user/AlexTelon
	create = group user/AlexTelon
	push = +force group user/AlexTelon
	pushMerge = group user/AlexTelon
[access "refs/tags/*"]
	push = +force group user/AlexTelon
	pushTag = group user/AlexTelon
	pushSignedTag = group user/AlexTelon
